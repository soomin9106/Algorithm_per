# 상하좌우
N = int(input())  # 공간의 크기
movePlan = input().split()  # 이동 계획
x, y = 1, 1

for i in range(len(movePlan)):
    if x <= N and y <= N:  # 공간 밖은 무시
        if movePlan[i] == 'L':
            if y > 1:
                y -= 1
        elif movePlan[i] == 'R':
            if y < N:
                y += 1
        elif movePlan[i] == 'U':
            if x > 1:
                x -= 1
        elif movePlan[i] == 'D':
            if x < N:
                x += 1

print(x, y)
